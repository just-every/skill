name = "demo-worker"
main = "src/index.ts"
compatibility_date = "2024-09-01"
compatibility_flags = ["nodejs_compat"]
send_metrics = false

workers_dev = true

[assets]
binding = "ASSETS"
directory = "../../apps/web/dist"

# This file is rendered by bootstrap.sh. Values wrapped in {{ }} are replaced with either
# explicit .env inputs or generated identifiers recorded in .env.local.generated.
[vars]
# --- Authentication ---
LOGTO_ISSUER = "https://login.justevery.com/oidc"
LOGTO_JWKS_URI = "https://login.justevery.com/oidc/jwks"
LOGTO_API_RESOURCE = "https://demo.justevery.com/api"
LOGTO_ENDPOINT = "https://login.justevery.com"
LOGTO_APPLICATION_ID = "2f8oaugvvlmg64p7vqyqw"
EXPO_PUBLIC_LOGTO_ENDPOINT = "https://login.justevery.com"
EXPO_PUBLIC_LOGTO_APP_ID = "2f8oaugvvlmg64p7vqyqw"
EXPO_PUBLIC_API_RESOURCE = "https://demo.justevery.com/api"
EXPO_PUBLIC_LOGTO_POST_LOGOUT_REDIRECT_URI = "https://demo.justevery.com"

# --- Application URLs ---
PROJECT_DOMAIN = "https://demo.justevery.com"
APP_BASE_URL = "/app"

# --- Stripe ---
STRIPE_PRODUCTS = "Founders:2500,usd,month;Scale:4900,usd,month"
EXPO_PUBLIC_WORKER_ORIGIN = "https://demo.justevery.com"

# --- Cloudflare metadata ---
CLOUDFLARE_ZONE_ID = "723318de8bc1f3353196efc02c4a09ed"

[[d1_databases]]
binding = "DB"
database_name = "demo-d1"
database_id = "7d2c53b8-05f4-4ba6-80b9-dc9b36114c51"

[[r2_buckets]]
binding = "STORAGE"
bucket_name = "demo-assets"

[[routes]]
pattern = "demo.justevery.com"
custom_domain = true

[observability]
enabled = false
