{
  "expo": {
    "name": "justevery-web",
    "slug": "justevery-web",
    "sdkVersion": "54.0.0",
    "scheme": "justevery",
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "entryPoint": "./index.ts",
    "web": {
      "bundler": "metro",
      "favicon": "./public/img/justevery-favicon-32x32.png"
    },
    "userInterfaceStyle": "automatic",
    "plugins": [
      "expo-secure-store"
    ],
    "ios": {
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    }
  }
}
